{
  "name": "cosmicvelocity/lumen-helpers",
  "description": "Improves interoperability between Lumen and Laravel.",
  "keywords": [
    "lumen",
    "laravel"
  ],
  "license": "Apache-2.0",
  "type": "library",
  "authors": [
    {
      "name": "Kouichi Machida",
      "email": "k-machida@aideo.co.jp"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "laravel/lumen-framework": ">=5.1"
  },
  "require-dev": {
    "mockery/mockery": "~0.9.0",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "CosmicVelocity\\LumenHelpers\\": "src/CosmicVelocity/LumenHelpers/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CosmicVelocity\\LumenHelpers\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "php vendor/bin/phpunit"
    ],
    "test-quiet": [
      "php vendor/bin/phpunit -d error_reporting=0"
    ]
  }
}
